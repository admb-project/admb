if [ ! -n "$ADMB_HOME" ]; then
    echo you must define the environment string ADMB_HOME
else
    g++ -w -o $1 -g $1.cpp -I. -fpermissive -I${ADMB_HOME}/include -L${ADMB_HOME}/lib -DUSE_LAPLACE -D__GNUDOS__ -Dlinux -DOPT_LIB -ldf1b2stub -ladmod -ladt -lado -ldf1b2stub -ladmod -ladt -lado
# replace line above with below for compiling from tpl2rem
#      -ldf1b2o -ladmod -ladt -lado -ldf1b2o -ladmod -ladt -lado
fi
