# for compiling from tpl2rem
if [ ! -n "$ADMB_HOME" ]; then
    echo you must define the environment string ADMB_HOME
else
    g++ -w $1.cpp -I. -fpermissive -I${ADMB_HOME}/include -L${ADMB_HOME}/lib -DUSE_LAPLACE -D__GNUDOS__ -Dlinux -ldf1b2s -ladmod -ladt -lads -ldf1b2s -ladmod -ladt -lads -o $1
fi
