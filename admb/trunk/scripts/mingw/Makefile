verify: ham4 ham5 catage finance vonb vonbr n2mvol simple
noverify: chem-eng forest pella-t truncreg

ham4:
	cd $(ADMB_HOME)\\examples\\admb\\buscycle && makeadm.bat ham4 && ham4.exe
	cd $(ADMB_HOME)\\examples\\admb\\buscycle && makeadms.bat ham4 && ham4.exe

ham5:
	cd $(ADMB_HOME)\\examples\\admb\\buscycle && makeadm.bat ham5 && ham5.exe
	cd $(ADMB_HOME)\\examples\\admb\\buscycle && makeadms.bat ham5 && ham5.exe

catage:
	cd $(ADMB_HOME)\\examples\\admb\\catage && makeadm.bat catage && catage.exe
	cd $(ADMB_HOME)\\examples\\admb\\catage && makeadms.bat catage && catage.exe

chem-eng:
	cd $(ADMB_HOME)\\examples\\admb\\chem-eng && makeadm.bat chem-eng && chem-eng.exe
	cd $(ADMB_HOME)\\examples\\admb\\chem-eng && makeadms.bat chem-eng && chem-eng.exe

finance:
	cd $(ADMB_HOME)\\examples\\admb\\finance && makeadm.bat finance && finance.exe
	cd $(ADMB_HOME)\\examples\\admb\\finance && makeadms.bat finance && finance.exe

forest:
	cd $(ADMB_HOME)\\examples\\admb\\forest && makeadm.bat forest && forest.exe
	cd $(ADMB_HOME)\\examples\\admb\\forest && makeadms.bat forest && forest.exe

pella-t:
	cd $(ADMB_HOME)\\examples\\admb\\pella-t && makeadm.bat pella-t && pella-t.exe
	cd $(ADMB_HOME)\\examples\\admb\\pella-t && makeadms.bat pella-t && pella-t.exe

vonb:
	cd $(ADMB_HOME)\\examples\\admb\\robreg && makeadm.bat vonb && vonb.exe
	cd $(ADMB_HOME)\\examples\\admb\\robreg && makeadms.bat vonb && vonb.exe

vonbr:
	cd $(ADMB_HOME)\\examples\\admb\\robreg && makeadm.bat vonbr && vonbr.exe
	cd $(ADMB_HOME)\\examples\\admb\\robreg && makeadms.bat vonbr && vonbr.exe

simple:
	cd $(ADMB_HOME)\\examples\\admb\\simple && makeadm.bat simple && simple.exe
	cd $(ADMB_HOME)\\examples\\admb\\simple && makeadms.bat simple && simple.exe

truncreg:
	cd $(ADMB_HOME)\\examples\\admb\\truncreg && makeadm.bat truncreg && runtrunc.bat && fruntrunc
	cd $(ADMB_HOME)\\examples\\admb\\truncreg && makeadms.bat truncreg && runtrunc.bat && fruntrunc

n2mvol:
	cd $(ADMB_HOME)\\examples\\admb\\vol && makeadm.bat n2mvol && n2mvol.exe
	cd $(ADMB_HOME)\\examples\\admb\\vol && makeadms.bat n2mvol && n2mvol.exe
