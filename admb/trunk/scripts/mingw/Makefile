verify: ham4 ham5 catage finance vonb vonbr n2mvol simple
noverify: chem-eng forest pella-t truncreg

ham4:
	cd
	cd examples/admb/buscycle && makeadm.bat ham4 && ./ham4.exe
	cd examples/admb/buscycle && makeadms.bat ham4 && ./ham4.exe

ham5:
	cd examples/admb/buscycle && makeadm.bat ham5 && ./ham5.exe
	cd examples/admb/buscycle && makeadms.bat ham5 && ./ham5.exe

catage:
	cd examples/admb/catage && makeadm.bat catage && ./catage.exe
	cd examples/admb/catage && makeadms.bat catage && ./catage.exe

chem-eng:
	cd examples/admb/chem-eng && makeadm.bat chem-eng && ./chem-eng.exe
	cd examples/admb/chem-eng && makeadms.bat chem-eng && ./chem-eng.exe

finance:
	cd examples/admb/finance && makeadm.bat finance && ./finance.exe
	cd examples/admb/finance && makeadms.bat finance && ./finance.exe

forest:
	cd examples/admb/forest && makeadm.bat forest && ./forest.exe
	cd examples/admb/forest && makeadms.bat forest && ./forest.exe

pella-t:
	cd examples/admb/pella-t && makeadm.bat pella-t && ./pella-t.exe
	cd examples/admb/pella-t && makeadms.bat pella-t && ./pella-t.exe

vonb:
	cd examples/admb/robreg && makeadm.bat vonb && ./vonb.exe
	cd examples/admb/robreg && makeadms.bat vonb && ./vonb.exe

vonbr:
	cd examples/admb/robreg && makeadm.bat vonbr && ./vonbr.exe
	cd examples/admb/robreg && makeadms.bat vonbr && ./vonbr.exe

simple:
	cd examples/admb/simple && makeadm.bat simple && ./simple.exe
	cd examples/admb/simple && makeadms.bat simple && ./simple.exe

truncreg:
	cd examples/admb/truncreg && makeadm.bat truncreg && runtrunc.bat && fruntrunc
	cd examples/admb/truncreg && makeadms.bat truncreg && runtrunc.bat && fruntrunc

n2mvol:
	cd examples/admb/vol && makeadm.bat n2mvol && ./n2mvol.exe
	cd examples/admb/vol && makeadms.bat n2mvol && ./n2mvol.exe
