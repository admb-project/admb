all:
	$(MAKE) --file=Makefile $(CXX)

dist:
	$(MAKE) --file=Makefile $(CXX)-dist

contrib:
	$(MAKE) --file=Makefile $(CXX)-contrib

verify:
	$(MAKE) --file=Makefile $(CXX)-verify

test:
	$(MAKE) --file=Makefile $(CXX)-tests

install:
	install -d build/dist @prefix@

clean:
	$(MAKE) --file=Makefile $(CXX)-clean
